{
  "name": "depinj-express-example",
  "version": "1.0.0",
  "description": "An express application using depinj",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.build.json",
    "dev": "LOCAL=true tsnd --respawn --files src/server.ts",
    "start": "node lib/server.js",
    "test": "yarn typecheck && yarn test:unit",
    "test:unit": "jest --config jest.unit.config.js",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sj-freitas/depinj-express-example/issues"
  },
  "homepage": "https://github.com/sj-freitas/depinj-express-example/tree/1.0.0#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "jest": "^26.4.0",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "depinj-js": "^1.5.0",
    "depinj-express": "^1.5.0",
    "express": "^4.17.1",
    "tslib": "^2.1.0"
  }
}
